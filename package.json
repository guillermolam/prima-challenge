{
  "name": "prisma",
  "version": "0.1.0",
  "bin": {
    "prisma": "bin/prisma.js"
  },
  "scripts": {
    "dev": "next",
    "build": "tsc && next build && npm run export",
    "bootstrap": "cdk bootstrap aws://401280197872/eu-west-3",
    "watch": "tsc -w",
    "cdk": "cdk",
    "cdk-deploy": "cdk deploy --all",
    "cdk-deploy-static": "npm run bootstrap && cdk deploy PrismaCapacitycStaticSite -y",
    "cdk-deploy-dynamic": "npm run bootstrap && cdk deploy PrismaCapacityDynamicSite -y",
    "cdk-destroy": "cdk destroy --all",
    "cdk-destroy-static": "npm run bootstrap && cdk destroy PrismaCapacitycStaticSite -y",
    "cdk-destroy-dynamic": "npm run bootstrap && cdk destroy PrismaCapacityDynamicSite -y",
    "cdk-synth": "cdk synth -v",
    "pulumi-deploy-static": "pulumi refresh -y && pulumi up -y -C ./infra-pulumi-static/",
    "pulumi-deploy-dynamic": "pulumi refresh -y && pulumi up -y -C ./infra-pulumi-dynamic/",
    "pulumi-destroy": "pulumi destroy -y",
    "start": "next start",
    "export": "next export"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.90.1",
    "@types/jest": "^26.0.20",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "aws-cdk": "1.90.1",
    "jest": "^26.4.2",
    "mime": "^2.5.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.90.1",
    "@aws-cdk/aws-cloudfront": "^1.90.1",
    "@aws-cdk/aws-route53": "^1.90.1",
    "@aws-cdk/aws-route53-targets": "^1.90.1",
    "@aws-cdk/aws-s3": "^1.90.1",
    "@aws-cdk/aws-s3-deployment": "^1.90.1",
    "@aws-cdk/core": "^1.90.1",
    "@pulumi/aws": "^3.30.0",
    "@pulumi/awsx": "^0.24.0",
    "@pulumi/pulumi": "^2.21.2",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "next": "^10.0.7",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
}
